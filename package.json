{
	"name": "reward-chart",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"start": "webpack-dev-server --mode development --open",
		"build": "webpack --mode production",
		"test": "jest"
	},
	"jest": {
		"verbose": true,
		"moduleNameMapper": {
			"\\.(css|less|scss)$": "<rootDir>/__mocks__/styleMock.js",
			"\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
		},
		"moduleFileExtensions": [
			"js",
			"jsx"
		],
		"moduleDirectories": [
			"node_modules"
		]
	},
	"dependencies": {
		"node-sass": "^4.9.0",
		"react": "^16.4.0",
		"react-dom": "^16.4.0",
		"react-scripts": "1.1.4",
		"sass-loader": "^7.0.3"
	},
	"devDependencies": {
		"babel-core": "^6.26.3",
		"babel-jest": "^23.0.1",
		"babel-loader": "^7.1.4",
		"babel-preset-env": "^1.7.0",
		"babel-preset-react": "^6.24.1",
		"css-loader": "^0.28.11",
		"html-webpack-plugin": "^3.2.0",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^23.1.0",
		"style-loader": "^0.21.0",
		"svg-inline-loader": "^0.8.0",
		"webpack": "^4.11.1",
		"webpack-cli": "^3.0.2",
		"webpack-dev-server": "^3.1.4"
	}
}
